<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            android:shape="oval"
            android:useLevel="false" >
            <solid android:color="@color/main_deep_gray" />

            <size
                android:height="15dp"
                android:width="15dp" />
        </shape>
    </item>
    <item
        android:left="1px"
        android:top="1px">
        <shape
            android:shape="oval"
            android:useLevel="false" >
            <solid android:color="@color/main_white" />

            <size
                android:height="15dp"
                android:width="15dp" />
        </shape>
    </item>

</layer-list>